{
  "name": "effect",
  "private": true,
  "repository": "https://github.com/Effect-TS/core.git",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "changeset": "changeset",
    "release": "changeset publish",
    "clean": "ultra -r clean && rimraf tsconfig.tsbuildinfo",
    "build": "yarn build-all && yarn build-pack",
    "build-pack": "ultra -r build-pack",
    "build-all": "tsc -b tsconfig.json",
    "build-watch": "tsc -b tsconfig.json --watch",
    "circular": "yarn org:madge --ts-config ./tsconfig.madge.json --circular --no-color --no-spinner --warning packages/*/build/esm packages/*/build/test packages/*/build/examples",
    "test": "vitest",
    "autofix": "ultra -r autofix",
    "lint": "ultra -r lint",
    "tc": "ultra -r tc",
    "postinstall": "tsplus-install",
    "org:rimraf": "cd $INIT_CWD && rimraf",
    "org:tsc": "cd $INIT_CWD && tsc",
    "org:eslint": "cd $INIT_CWD && eslint",
    "org:concurrently": "cd $INIT_CWD && concurrently",
    "org:build-utils": "cd $INIT_CWD && build-utils",
    "org:babel": "cd $INIT_CWD && babel",
    "org:madge": "cd $INIT_CWD && madge"
  },
  "packageManager": "yarn@3.1.1",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@changesets/changelog-github": "^0.4.4",
    "@changesets/cli": "^2.22.0",
    "@effect-ts/build-utils": "0.39.3",
    "@effect-ts/core": "^0.59.3",
    "@phaphoso/eslint-plugin-dprint": "^0.5.2",
    "@tsplus/installer": "0.0.81",
    "@tsplus/stdlib": "0.0.43",
    "@types/node": "^17.0.23",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-replace-import-extension": "^1.1.2",
    "c8": "^7.11.0",
    "concurrently": "^7.1.0",
    "cpx": "^1.5.0",
    "dprint": "^0.24.4",
    "eslint": "^8.13.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-codegen": "0.16.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "madge": "^5.0.1",
    "picocolors": "^1.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "ultra-runner": "^3.10.5",
    "vite": "^2.9.1",
    "vitest": "0.9.2"
  },
  "resolutions": {
    "eslint-plugin-codegen": "patch:eslint-plugin-codegen@npm:0.16.1#.yarn/patches/eslint-plugin-codegen-npm-0.16.1-87770191cd",
    "@effect-ts/build-utils": "patch:@effect-ts/build-utils@npm:0.39.2#.yarn/patches/@effect-ts-build-utils-npm-0.39.2-f802d4baa8"
  }
}
