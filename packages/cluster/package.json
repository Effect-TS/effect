{
  "name": "@effect/cluster",
  "type": "module",
  "version": "0.30.3",
  "description": "Unified interfaces for common cluster-specific services",
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "directory": "dist",
    "linkDirectory": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Effect-TS/effect.git",
    "directory": "packages/cluster"
  },
  "homepage": "https://effect.website",
  "license": "MIT",
  "main": "./src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./ClusterError": "./src/ClusterError.ts",
    "./ClusterMetrics": "./src/ClusterMetrics.ts",
    "./ClusterSchema": "./src/ClusterSchema.ts",
    "./DeliverAt": "./src/DeliverAt.ts",
    "./Entity": "./src/Entity.ts",
    "./EntityAddress": "./src/EntityAddress.ts",
    "./EntityId": "./src/EntityId.ts",
    "./EntityType": "./src/EntityType.ts",
    "./Envelope": "./src/Envelope.ts",
    "./HttpCommon": "./src/HttpCommon.ts",
    "./HttpRunner": "./src/HttpRunner.ts",
    "./HttpShardManager": "./src/HttpShardManager.ts",
    "./MachineId": "./src/MachineId.ts",
    "./Message": "./src/Message.ts",
    "./MessageStorage": "./src/MessageStorage.ts",
    "./Reply": "./src/Reply.ts",
    "./Runner": "./src/Runner.ts",
    "./RunnerAddress": "./src/RunnerAddress.ts",
    "./RunnerHealth": "./src/RunnerHealth.ts",
    "./RunnerServer": "./src/RunnerServer.ts",
    "./Runners": "./src/Runners.ts",
    "./ShardId": "./src/ShardId.ts",
    "./ShardManager": "./src/ShardManager.ts",
    "./ShardStorage": "./src/ShardStorage.ts",
    "./Sharding": "./src/Sharding.ts",
    "./ShardingConfig": "./src/ShardingConfig.ts",
    "./ShardingRegistrationEvent": "./src/ShardingRegistrationEvent.ts",
    "./Singleton": "./src/Singleton.ts",
    "./SingletonAddress": "./src/SingletonAddress.ts",
    "./Snowflake": "./src/Snowflake.ts",
    "./SocketRunner": "./src/SocketRunner.ts",
    "./SocketShardManager": "./src/SocketShardManager.ts",
    "./SqlMessageStorage": "./src/SqlMessageStorage.ts",
    "./SqlShardStorage": "./src/SqlShardStorage.ts",
    "./SynchronizedClock": "./src/SynchronizedClock.ts"
  },
  "scripts": {
    "codegen": "build-utils prepare-v2",
    "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v2",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "peerDependencies": {
    "@effect/platform": "workspace:^",
    "@effect/rpc": "workspace:^",
    "@effect/sql": "workspace:^",
    "effect": "workspace:^"
  },
  "devDependencies": {
    "@effect/platform": "workspace:^",
    "@effect/platform-node": "workspace:^",
    "@effect/rpc": "workspace:^",
    "@effect/sql": "workspace:^",
    "@effect/sql-sqlite-node": "workspace:^",
    "@testcontainers/mysql": "^10.18.0",
    "@testcontainers/postgresql": "^10.18.0",
    "effect": "workspace:^"
  }
}
