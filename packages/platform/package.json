    "./HttpClientRequest": "./src/HttpClientRequest.ts",
    "./HttpClientResponse": "./src/HttpClientResponse.ts",
    "./HttpIncomingMessage": "./src/HttpIncomingMessage.ts",
    "./HttpMethod": "./src/HttpMethod.ts",
    "./HttpMiddleware": "./src/HttpMiddleware.ts",
    "./HttpMultiplex": "./src/HttpMultiplex.ts",
    "./HttpPlatform": "./src/HttpPlatform.ts",
    "./HttpRouter": "./src/HttpRouter.ts",
    "./HttpServer": "./src/HttpServer.ts",
    "./HttpServerError": "./src/HttpServerError.ts",
    "./HttpServerRequest": "./src/HttpServerRequest.ts",
    "./HttpServerRespondable": "./src/HttpServerRespondable.ts",
