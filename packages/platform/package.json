    "./HttpApi": "./src/HttpApi.ts",
    "./HttpApiBuilder": "./src/HttpApiBuilder.ts",
    "./HttpApiClient": "./src/HttpApiClient.ts",
    "./HttpApiEndpoint": "./src/HttpApiEndpoint.ts",
    "./HttpApiError": "./src/HttpApiError.ts",
    "./HttpApiGroup": "./src/HttpApiGroup.ts",
