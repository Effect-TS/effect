{
  "name": "@effect-ts/system-next",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/Effect-TS/core.git",
  "homepage": "https://www.matechs.com",
  "scripts": {
    "clean": "yarn ets:rimraf build tsbuildinfo dist",
    "build-esm": "yarn ets:tsc -p tsconfig.build.json",
    "build-cjs": "sh build-cjs.sh",
    "build": "yarn build-esm && yarn build-cjs",
    "lint": "yarn ets:eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "yarn ets:prettier --write \"./{src,test,demo}/**/*.ts\"",
    "demo": "yarn ets:ts-node -O '{\"module\":\"commonjs\"}' --project tsconfig.build.json"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "exports": {
    "./Cause": {
      "require": "./build/cjs/Cause/index.js",
      "import": "./build/esm/Cause/index.js"
    },
    "./Cause/operations": {
      "require": "./build/cjs/Cause/operations/index.js",
      "import": "./build/esm/Cause/operations/index.js"
    },
    "./*": {
      "require": "./build/cjs/*.js",
      "import": "./build/esm/*.js"
    }
  }
}
