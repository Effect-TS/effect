{
  "name": "@matechs/test",
  "version": "4.10.8",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/mikearnaldi/matechs-effect.git",
  "scripts": {
    "clean": "rimraf build tsbuildinfo",
    "build:lib": "tsc --build tsconfig.build.json && ts-node ../../scripts/package.ts",
    "build:es6": "tsc --build tsconfig.es6.json",
    "build": "yarn run build:lib && yarn run build:es6 && ts-node ../../scripts/rewrite-locals.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,test,demo}/**/*.ts\"",
    "tc": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "sideEffects": false,
  "gitHead": "2ff0a2daa194950901aba13f3431b2470889f9c2",
  "dependencies": {
    "@matechs/core": "^2.10.8",
    "fast-check": "^1.24.1"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1"
  },
  "config": {
    "modules": [
      "Aspects",
      "Aspects/Hook",
      "Aspects/Provider",
      "Aspects/Retry",
      "Aspects/Skip",
      "Aspects/Timeout",
      "Aspects/Todo",
      "Config",
      "Def",
      "Impl",
      "FC",
      "Mock"
    ]
  }
}
