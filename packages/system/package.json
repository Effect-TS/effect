{
  "name": "@effect-ts/system",
  "version": "0.23.4",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/Effect-TS/system.git",
  "homepage": "https://www.matechs.com",
  "scripts": {
    "clean": "rimraf build tsbuildinfo",
    "build:lib": "ttsc --build  tsconfig.build.json && ts-node ../../scripts/package.ts",
    "build:traced:lib": "ttsc --build  tsconfig.traced.build.json && ts-node ../../scripts-traced/package.ts",
    "build:es6": "ttsc --build  tsconfig.es6.json && ts-node ../../scripts/rewrite-esm.ts && babel build/esm/ -d build/esm/ -s --plugins annotate-pure-calls && ts-node ../../scripts/package-esm.ts",
    "build:traced:es6": "ttsc --build  tsconfig.traced.es6.json && ts-node ../../scripts-traced/rewrite-esm.ts && babel build/_traced/esm/ -d build/_traced/esm/ -s --plugins annotate-pure-calls && ts-node ../../scripts-traced/package-esm.ts",
    "build:post-process": "ts-node ../../scripts/rewrite-source-maps.ts && ts-node ../../scripts-traced/rewrite-source-maps.ts && ts-node ../../scripts/rewrite-locals.ts && ts-node ../../scripts-traced/rewrite-locals.ts && yarn prettier:lib",
    "build": "yarn run build:lib && yarn run build:traced:lib && yarn run build:es6 && yarn run build:traced:es6 && yarn run build:post-process",
    "lint": "eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,test,demo}/**/*.ts\"",
    "prettier:lib": "prettier --write \"./build/**/*.js\"",
    "tc": "tsc --noEmit",
    "demo": "ts-node -r tsconfig-paths/register",
    "circular": "madge --circular --ts-config ./tsconfig.build.json --extensions ts ./src"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "sideEffects": false,
  "dependencies": {
    "@effect-ts/tracing-utils": "^0.4.3"
  },
  "config": {
    "side": [
      "Operator"
    ],
    "modules": [
      "Array",
      "Chunk",
      "Async",
      "Cause",
      "Cause/Pretty",
      "Clock",
      "Dictionary",
      "Effect",
      "Either",
      "Equal",
      "Exit",
      "Fiber",
      "FiberRef",
      "FreeAssociative",
      "Function",
      "GlobalExceptions",
      "Has",
      "Hash",
      "IO",
      "Iterable",
      "Layer",
      "Persistent",
      "Persistent/List",
      "Persistent/HashMap",
      "Persistent/HashMap/Array",
      "Persistent/HashMap/Bitwise",
      "Persistent/HashMap/Config",
      "Persistent/HashMap/Nodes",
      "Persistent/HashSet",
      "Persistent/RedBlackTree",
      "Persistent/SortedSet",
      "Managed",
      "Managed/ReleaseMap",
      "Map",
      "Mutable",
      "NonEmptyArray",
      "Operator",
      "Option",
      "Ord",
      "Ordering",
      "Promise",
      "Queue",
      "Random",
      "Ref",
      "RefM",
      "Schedule",
      "Scope",
      "Semaphore",
      "Stack",
      "Stream",
      "Stream/_internal",
      "Stream/BufferedPull",
      "Stream/Pull",
      "Stream/Push",
      "Stream/Sink",
      "Stream/Stream",
      "Stream/Take",
      "Stream/Transducer",
      "Supervisor",
      "Support",
      "Support/AtomicBoolean",
      "Support/AtomicNumber",
      "Support/AtomicReference",
      "Support/DoublyLinkedList",
      "Support/ImmutableQueue",
      "Support/LinkedList",
      "Support/MutableQueue",
      "Support/OneShot",
      "Support/RingBuffer",
      "Support/Scheduler",
      "Sync",
      "Utils",
      "XPure"
    ]
  }
}
