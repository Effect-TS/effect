{
  "name": "@effect/platform-bun",
  "type": "module",
  "version": "0.61.3",
  "license": "MIT",
  "description": "Platform specific implementations for the Bun runtime",
  "homepage": "https://effect.website",
  "repository": {
    "type": "git",
    "url": "https://github.com/Effect-TS/effect.git",
    "directory": "packages/platform-bun"
  },
  "bugs": {
    "url": "https://github.com/Effect-TS/effect/issues"
  },
  "tags": [
    "bun",
    "typescript",
    "algebraic-data-types",
    "functional-programming"
  ],
  "keywords": [
    "bun",
    "typescript",
    "algebraic-data-types",
    "functional-programming"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "directory": "dist",
    "linkDirectory": false
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./BunClusterRunnerHttp": "./src/BunClusterRunnerHttp.ts",
    "./BunClusterRunnerSocket": "./src/BunClusterRunnerSocket.ts",
    "./BunClusterShardManagerHttp": "./src/BunClusterShardManagerHttp.ts",
    "./BunClusterShardManagerSocket": "./src/BunClusterShardManagerSocket.ts",
    "./BunCommandExecutor": "./src/BunCommandExecutor.ts",
    "./BunContext": "./src/BunContext.ts",
    "./BunFileSystem": "./src/BunFileSystem.ts",
    "./BunFileSystem/ParcelWatcher": "./src/BunFileSystem/ParcelWatcher.ts",
    "./BunHttpPlatform": "./src/BunHttpPlatform.ts",
    "./BunHttpServer": "./src/BunHttpServer.ts",
    "./BunHttpServerRequest": "./src/BunHttpServerRequest.ts",
    "./BunKeyValueStore": "./src/BunKeyValueStore.ts",
    "./BunMultipart": "./src/BunMultipart.ts",
    "./BunPath": "./src/BunPath.ts",
    "./BunRuntime": "./src/BunRuntime.ts",
    "./BunSink": "./src/BunSink.ts",
    "./BunSocket": "./src/BunSocket.ts",
    "./BunSocketServer": "./src/BunSocketServer.ts",
    "./BunStream": "./src/BunStream.ts",
    "./BunTerminal": "./src/BunTerminal.ts",
    "./BunWorker": "./src/BunWorker.ts",
    "./BunWorkerRunner": "./src/BunWorkerRunner.ts"
  },
  "scripts": {
    "codegen": "build-utils prepare-v2",
    "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v2",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "peerDependencies": {
    "@effect/cluster": "workspace:^",
    "@effect/platform": "workspace:^",
    "@effect/rpc": "workspace:^",
    "@effect/sql": "workspace:^",
    "effect": "workspace:^"
  },
  "dependencies": {
    "@effect/platform-node-shared": "workspace:^",
    "multipasta": "^0.2.5"
  },
  "devDependencies": {
    "@effect/cluster": "workspace:^",
    "@effect/platform": "workspace:^",
    "@effect/rpc": "workspace:^",
    "@effect/sql": "workspace:^",
    "@types/bun": "^1.2.2",
    "effect": "workspace:^"
  }
}
