{
  "name": "@effect-native/debug",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "description": "JS runtime debugger protocol abstraction",
  "bin": {
    "debug-steps": "./bin/debug-steps.js"
  },
  "homepage": "https://github.com/effect-native/effect-native",
  "repository": {
    "type": "git",
    "url": "https://github.com/effect-native/effect-native.git",
    "directory": "packages-native/debug"
  },
  "bugs": {
    "url": "https://github.com/effect-native/effect-native/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "directory": "dist",
    "linkDirectory": false,
    "tag": "latest"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./Debug": "./src/Debug.ts",
    "./cli/steps": "./src/cli/steps.ts",
    "./internal/*": null
  },
  "scripts": {
    "codegen": "echo '@effect-native/debug: no codegen'",
    "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v3 && pnpm copy-bin",
    "copy-bin": "mkdir -p dist/bin && cp bin/debug-steps-dist.js dist/bin/debug-steps.js && chmod +x dist/bin/debug-steps.js && node -e \"const fs=require('fs');const p=JSON.parse(fs.readFileSync('dist/package.json','utf8'));p.type='module';fs.writeFileSync('dist/package.json',JSON.stringify(p,null,2)+'\\n')\"",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "test:debug-log-steps": "bash bin/test-steps.sh 50"
  },
  "dependencies": {
    "@effect/cli": "^0.58.1",
    "@effect/platform-node": "^0.97.1"
  },
  "peerDependencies": {
    "@effect/platform": "^0.91.1",
    "effect": "^3.17.14"
  },
  "devDependencies": {
    "@effect/platform": "^0.91.1",
    "@effect/vitest": "^0.25.1",
    "effect": "^3.17.14",
    "tsx": "^4.19.0",
    "ws": "^8.18.2",
    "@types/ws": "^8.18.1"
  }
}
