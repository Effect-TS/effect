{
  "$schema": "../../node_modules/@effect/docgen/schema.json",
  "include": ["src/**/*.ts"],
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2022",
    "target": "ES2022",
    "lib": ["ES2022"],
    "paths": {
      "@effect-native/libcrsql": ["./src/index.ts"],
      "@effect-native/libcrsql/effect": ["./src/effect.ts"],
      "@effect-native/libcrsql/paths": ["./src/paths.ts"],
      "effect": ["../../packages/effect/src/index.js"],
      "effect/*": ["../../packages/effect/src/*.js"]
    }
  }
}

