{
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "preserveSymlinks": true,
    "moduleResolution": "node",
    "noEmit": false,
    "lib": ["es2018"],
    "sourceMap": true,
    "strictFunctionTypes": true,
    "strict": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true,
    "stripInternal": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@matechs/core/*": ["packages/core/build/*"],
      "@matechs/core": ["packages/core/build"],
      "@matechs/contrib/*": ["packages/contrib/build/*"],
      "@matechs/contrib": ["packages/contrib/build"],
      "@matechs/test/*": ["packages/test/build/*"],
      "@matechs/test": ["packages/test/build"],
      "@matechs/test-jest/*": ["packages/test-jest/build/*"],
      "@matechs/test-jest": ["packages/test-jest/build"],
      "@matechs/aio/*": ["packages/aio/build/*"],
      "@matechs/aio": ["packages/aio/build"]
    }
  },
  "exclude": ["node_modules", "lib"]
}
