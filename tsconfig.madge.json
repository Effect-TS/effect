{
    "compilerOptions": {
        "noEmit": true,
        "allowJs": true,
        "baseUrl": ".",
        "paths": {
            "@effect/core": [
                "./packages/core/build/esm/index.js"
            ],
            "@effect/core/test/*": [
                "./packages/core/build/test/*"
            ],
            "@effect/core/examples/*": [
                "./packages/core/build/examples/*"
            ],
            "@effect/core/*": [
                "./packages/core/build/esm/*"
            ],
        }
    },
    "include": [
        "./packages/core/build/esm/**/*.js",
        "./packages/core/build/test/**/*.js",
        "./packages/core/build/examples/**/*.js"
    ]
}
