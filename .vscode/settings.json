{
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "files.exclude": {
    "dist": true,
    "Bigint": true,
    "Either": true,
    "HashMap": true,
    "MutableRef": true,
    "Predicate": true,
    "Symbol": true,
    "Boolean": true,
    "Encoding": true,
    "HashSet": true,
    "NonEmptyIterable": true,
    "ReadonlyArray": true,
    "Tuple": true,
    "Brand": true,
    "Equal": true,
    "Inspectable": true,
    "Number": true,
    "ReadonlyRecord": true,
    "Types": true,
    "Chunk": true,
    "Equivalence": true,
    "List": true,
    "Option": true,
    "RedBlackTree": true,
    "Unify": true,
    "Context": true,
    "Function": true,
    "MutableHashMap": true,
    "Order": true,
    "SortedMap": true,
    "Utils": true,
    "Data": true,
    "GlobalValue": true,
    "MutableHashSet": true,
    "Ordering": true,
    "SortedSet": true,
    "Differ": true,
    "HKT": true,
    "MutableList": true,
    "String": true,
    "Duration": true,
    "Hash": true,
    "MutableQueue": true,
    "Pipeable": true,
    "Struct": true,
    "Cache": true,
    "Hub": true,
    "Request": true,
    "Cause": true,
    "KeyedPool": true,
    "RequestBlock": true,
    "Clock": true,
    "Layer": true,
    "RequestResolver": true,
    "LogLevel": true,
    "Resource": true,
    "Config": true,
    "LogSpan": true,
    "Runtime": true,
    "ConfigError": true,
    "Logger": true,
    "RuntimeFlags": true,
    "ConfigProvider": true,
    "Metric": true,
    "RuntimeFlagsPatch": true,
    "ConfigProviderPathPatch": true,
    "MetricBoundaries": true,
    "Schedule": true,
    "ConfigSecret": true,
    "MetricHook": true,
    "ScheduleDecision": true,
    "Console": true,
    "MetricKey": true,
    "ScheduleInterval": true,
    "DefaultServices": true,
    "MetricKeyType": true,
    "ScheduleIntervals": true,
    "Deferred": true,
    "MetricLabel": true,
    "Scheduler": true,
    "Effect": true,
    "MetricPair": true,
    "Scope": true,
    "ExecutionStrategy": true,
    "MetricPolling": true,
    "ScopedCache": true,
    "Exit": true,
    "MetricRegistry": true,
    "ScopedRef": true,
    "Fiber": true,
    "MetricState": true,
    "Supervisor": true,
    "FiberId": true,
    "Pool": true,
    "SynchronizedRef": true,
    "FiberRef": true,
    "Queue": true,
    "Tracer": true,
    "FiberRefs": true,
    "Random": true,
    "FiberRefsPatch": true,
    "Ref": true,
    "FiberStatus": true,
    "Reloadable": true,
    "STM": true,
    "THub": true,
    "TQueue": true,
    "TRef": true,
    "TArray": true,
    "TMap": true,
    "TRandom": true,
    "TSemaphore": true,
    "TDeferred": true,
    "TPriorityQueue": true,
    "TReentrantLock": true,
    "TSet": true,
    "TestAnnotation": true,
    "TestAnnotationMap": true,
    "TestAnnotations": true,
    "TestClock": true,
    "TestConfig": true,
    "TestContext": true,
    "TestLive": true,
    "TestServices": true,
    "TestSized": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "eslint.validate": ["markdown", "javascript", "typescript"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.quickSuggestionsDelay": 10,
  "editor.suggestOnTriggerCharacters": true,
  "editor.tabCompletion": "off",
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "recentlyUsed",
  "editor.wordBasedSuggestions": true,
  "editor.parameterHints.enabled": true,
  "files.watcherExclude": {
    "**/target": true
  },
  "files.insertFinalNewline": true
}
